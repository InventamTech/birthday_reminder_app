{
	"name": "reminder-app-nodejs",
	"version": "0.0.1",
	"description": "Fulfillment for Hubspot google action and alexa skill",
	"main": "index.js",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"scripts": {
		"precommit": "pretty-quick --write {,*/**/}*.{ts,json} --staged",
		"commitmsg": "commitlint -e $GIT_PARAMS",
		"commit": "git-cz",
		"release": "standard-version --no-verify",
		"build": "tsc",
		"clean": "npx rimraf ./build",
		"start": "node build/server.js",
		"dist": "sh ./scripts/build.sh",
		"server": "tsc-watch --outDir ./build --onSuccess \"npm run server -- --mode=http\" --onFailure \"echo ERROR! Compilation Failed\""
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write --trailing-comma es6 --parser typescript",
			"git add"
		]
	},
	"author": "Gautam",
	"license": "ISC",
	"dependencies": {
		"ask-sdk-core": "^2.4.0",
		"ask-sdk-model": "^1.12.0",
		"aws-sdk": "^2.567.0",
		"axios": "^0.19.2",
		"body-parser": "^1.19.0",
		"config": "^3.3.2",
		"cors": "^2.8.5",
		"cron": "^1.4.1",
		"csv-parse": "^4.15.3",
		"express": "^4.17.1",
		"form-data": "3.0.0",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"md5": "^2.3.0",
		"moment": "^2.24.0",
		"moment-timezone": "^0.5.28",
		"mongoose": "5.10.1",
		"multer": "1.4.2",
		"mustache": "^4.0.1",
		"node-cron": "^3.0.0",
		"nodemailer": "^6.5.0",
		"pg": "^7.12.1",
		"pg-hstore": "^2.3.3",
		"redis": "^3.0.2",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^5.21.5",
		"sequelize-typescript": "^1.1.0",
		"socket.io": "^2.3.0",
		"swagger-ui-express": "^4.1.4",
		"twilio": "^3.41.1",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@types/aws-sdk": "^2.7.0",
		"@types/bluebird": "^3.5.31",
		"@types/config": "0.0.34",
		"@types/cors": "^2.8.6",
		"@types/cron": "^1.3.0",
		"@types/express": "^4.17.11",
		"@types/lodash": "^4.14.120",
		"@types/moment": "^2.13.0",
		"@types/moment-timezone": "^0.5.13",
		"@types/mongoose": "5.10.1",
		"@types/multer": "1.4.3",
		"@types/node": "^12.0.0",
		"@types/redis": "^2.8.18",
		"@types/request": "^2.47.1",
		"@types/socket.io": "^2.1.11",
		"commitizen": "^4.2.2",
		"cz-conventional-changelog": "^3.3.0",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.2",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.1.0",
		"tsc-watch": "^4.2.9",
		"tslint": "^5.12.1",
		"typescript": "~3.7.2"
	},
	"homepage": "https://gitlab.com/voxxup-gti/reminder-app-nodejs.git#readme"
}